<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dhiraj.focus2">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".camList"
            android:label="@string/title_activity_camera_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CameraAdd" />
        <activity android:name=".ModeList" />
        <activity android:name=".AddMode" />
        <activity android:name=".display" />
        <activity android:name=".ToGallary" />
        <activity android:name=".fullImage"></activity>
    </application>

</manifest>